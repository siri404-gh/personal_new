{
  "name": "sreeram.pro",
  "version": "1.0.0",
  "description": "Sreeram Padmanabhan's portfolio website",
  "main": "index.js",
  "scripts": {
    "flow": "./node_modules/.bin/flow --json --pretty --quiet",
    "lint": "./node_modules/.bin/eslint .  -o=./.dist/reports/.eslint/index.html -f=html",
    "test": "./node_modules/.bin/karma start",
    "report-coverage": "./node_modules/.bin/codecov -t $CODECOV_KEY",
    "docs": "./node_modules/.bin/esdoc -c=./.esdoc.js",
    "pa11y": "mkdir -p ./.dist/reports/.pa11y && ./node_modules/.bin/pa11y-ci --config .pa11yrc --threshold 5 --json > ./.dist/reports/.pa11y/output.json",
    "sitespeed": "./node_modules/.bin/sitespeed.io --outputFolder=./.dist/reports/.sitespeed --config=.sitespeed.json .sitespeed.txt",
    "cucumber": "mkdir -p .dist/reports/.cucumber && CONFIG_FILE=single ./node_modules/cucumber/bin/cucumber-js bdd/features/single.feature -f json:.dist/reports/.cucumber/output.json && node cucumber-reporter",
    "build": "./node_modules/.bin/webpack --config=webpack/webpack.config.js --env=prod --progress --profile --colors",
    "build:dev": "./node_modules/.bin/webpack-dev-server --config=webpack/webpack.config.js --env=dev --progress --profile --colors --open",
    "cleanup": "./node_modules/.bin/rimraf android/app/src/main/assets android ios output TMPDIR/react-* node_modules && yarn",
    "eject": "react-native eject",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle:mobile": "yarn cleanup && yarn eject && mkdir android/app/src/main/assets && yarn bundle && yarn cleanup",
    "start:mobile:server": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "start:mobile:dev": "react-native run-android",
    "start:mobile:dev:ios": "react-native run-ios"
  },
  "repository": "git+https://github.com/sreerampr/sreerampr.github.io.git",
  "keywords": [
    "sreeram",
    "padmanabhan",
    "web",
    "developer",
    "designer",
    "traveller",
    "instagram",
    "london",
    "india",
    "photographer"
  ],
  "author": "Sreeram Padmanabhan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sreerampr/sreerampr.github.io/issues"
  },
  "homepage": "https://github.com/sreerampr/sreerampr.github.io#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-native": "^4.0.0",
    "css-loader": "^0.28.9",
    "expect": "^22.1.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.52.2",
    "react-redux": "^5.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-flow": "^6.23.0",
    "body-parser": "^1.18.2",
    "browserstack-local": "^1.3.0",
    "clean-webpack-plugin": "^0.1.18",
    "codecov": "^3.0.0",
    "cucumber": "^4.0.0",
    "cucumber-assert": "^2.0.0",
    "cucumber-html-reporter": "^4.0.0",
    "esdoc": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.16.0",
    "express": "^4.16.2",
    "firebase-tools": "^3.17.4",
    "flow": "^0.2.3",
    "flow-bin": "^0.64.0",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.9.1",
    "jest": "^22.1.4",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.9",
    "morgan": "^1.9.0",
    "pa11y": "^4.13.2",
    "pa11y-ci": "^1.3.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "2.53.2",
    "sitespeed.io": "^5.6.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/src/components/mobile/**/*.test.js?(x)"
    ]
  },
  "engines": {
    "node": "7.6.0",
    "npm": "4.1.2"
  }
}